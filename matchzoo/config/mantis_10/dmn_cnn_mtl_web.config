{
   "inputs": {
      "predict": {
         "phase": "PREDICT",
         "input_type": "DMN_ListGenerator",
         "relation_file": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/relation_test.txt.fd",
         "batch_list": 50
      },
      "predict_web": {
         "phase": "PREDICT",
         "input_type": "DMN_ListGenerator",
         "relation_file": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/relation_test_web.txt.fd",
         "batch_list": 50
      },
      "train": {
         "phase": "TRAIN",
         "relation_file": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/relation_train.txt.fd",
         "input_type": "DMN_PairGenerator",
         "batch_size": 50,
         "batch_per_iter": 1,
         "query_per_iter": 1000,
         "use_iter": false,
         "use_intents": false
      },
      "train_web": {
         "phase": "TRAIN",
         "relation_file": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/relation_train_web.txt.fd",
         "input_type": "DMN_PairGenerator",
         "batch_size": 50,
         "batch_per_iter": 1,
         "query_per_iter": 1000,
         "use_iter": false,
         "use_intents": false
      },
      "share": {
         "predict": true,
         "balanced_domain_batches": true,
         "vocab_size": 429495,
         "use_dpool": false,
         "embed_size": 200,
         "target_mode": "ranking",
         "text1_corpus": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/corpus_preprocessed.txt",
         "text2_corpus": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/corpus_preprocessed.txt",
         "embed_path": "../data/mantis_10/ModelInput_mtl_web/cut_embed_mikolov_200d_no_readvocab.txt",
         "text1_maxlen": 50,
         "text1_max_utt_num": 5,
         "train_embed": true,
         "text2_maxlen": 50,
         "samples_per_context": 11
      },
      "valid": {
         "phase": "EVAL",
         "input_type": "DMN_ListGenerator",
         "relation_file": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/relation_valid.txt.fd",
         "batch_list": 50
      },
      "valid_web": {
         "phase": "EVAL",
         "input_type": "DMN_ListGenerator",
         "relation_file": "../data/mantis_10/ModelInput_mtl_web/dmn_model_input/relation_valid_web.txt.fd",
         "batch_list": 50
      }
   },
   "global": {
      "optimizer": "adam",
      "num_iters": 500,
      "save_weights_iters": 50,
      "learning_rate": 0.001,
      "test_weights_iters": 500,
      "weights_file": "../data/mantis_10/ModelRes/dmn_cnn_mtl_web.weights",
      "weights_file_web": "../data/mantis_10/ModelRes/dmn_cnn_mtl_web_web.weights",
      "num_batch": 100,
      "num_epochs": 100,
      "model_type": "PY",
      "display_interval": 10
   },
   "outputs": {
      "predict": {
         "save_format": "TREC",
         "save_path": "../data/mantis_10/ModelRes/dmn_cnn_mtl_web.predict.test.txt",
         "save_path_web": "../data/mantis_10/ModelRes/dmn_cnn_mtl_web_web.predict.test.txt"
      }
   },
  "losses": [
      {
         "object_name": "rank_hinge_loss",
         "object_params": {
            "margin": 1.0
         }
      }
   ],
   "metrics": [
      "map",
      "ndcg@10"
   ],
   "net_name": "DMN_CNN_MTL_Web",
   "model": {
      "model_py": "dmn_cnn_mtl_web.DMN_CNN_MTL_Web",
      "setting": {
         "dropout_rate": 0.3,
         "hidden_size": 200,
         "2d_mpool_sizes": [
            [
               3,3
            ]
         ],
         "2d_kernel_sizes": [
            [
               3,3
            ]
         ],
         "num_conv2d_layers": 1,
         "2d_kernel_counts": [8]
